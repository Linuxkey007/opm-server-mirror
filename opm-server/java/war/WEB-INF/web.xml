<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Opm</servlet-name>
		<servlet-class>opm.OpmServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UcwebServlet</display-name>
		<servlet-name>UcwebServlet</servlet-name>
		<servlet-class>opm.UcwebServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Opm</servlet-name>
		<url-pattern>/opm</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UcwebServlet</servlet-name>
		<url-pattern>/ucweb</url-pattern>
	</servlet-mapping>
</web-app>
